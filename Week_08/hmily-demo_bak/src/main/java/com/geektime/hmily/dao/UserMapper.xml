<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.geektime.hmily.dao.UserMapper">
    <select id="list" resultType="com.geektime.hmily.pojo.entity.User">
        select * from tb_user;
    </select>

    <insert id="save" parameterType="com.geektime.hmily.pojo.entity.User">
        insert into tb_user(card_id,name) values (#{user.cardId},#{user.name})
    </insert>
</mapper>